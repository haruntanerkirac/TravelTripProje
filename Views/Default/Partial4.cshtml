@using TravelTripProje.Models.Sınıflar
@model List<Blog>

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">
                <img src="@item.BlogImage" style="width: 120px; height: 65px;" alt="" />
            </div>
            <div class="bot-desc">
                <p>@item.Baslik </p>
                <p>
                    <a href="/Blog/BlogDetay/@item.ID">Okumak için tıkla..</a>
                </p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5","Default")
</div>
